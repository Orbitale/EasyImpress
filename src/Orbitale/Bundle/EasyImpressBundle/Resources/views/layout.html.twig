<!DOCTYPE html>
<html lang="fr">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta name="generator" content="Orbitale EasyImpress">
    <meta name="description" content="{% block description %}{% endblock %}">
    <meta name="author" content='Alex "Pierstoval" Rock Ancelet <pierstoval@gmail.com>'>
    <link rel="apple-touch-icon-precomposed" href="{{ asset('favicon.ico') }}">
    <link rel="icon" href="{{ asset('favicon.ico') }}">
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}">

    <title>{% block title %}{% endblock %}</title>

    <link href="{{ asset('bundles/easyimpress/css/bootstrap.min.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/easyimpress/css/main.css') }}" rel="stylesheet">
    <link href="{{ asset('css/slides.css') }}" rel="stylesheet">
    <script src="{{ asset('bundles/easyimpress/js/ie10-viewport-bug-workaround.js') }}" type="text/javascript"></script>

    <meta property="og:title" content="My Impress porfolio by Pierstoval">

    {% block css %}{% endblock %}

    <style type="text/css" id="holderjs-style"></style>
</head>

<body class="{% block bodyClass %}{% endblock %}">

{% block easy_impress_presentation %}
{% endblock %}

{% block js %}
    <script src="{{ asset('bundles/easyimpress/js/jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/easyimpress/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/easyimpress/js/jquery.transit.min.js') }}"></script>
{% endblock %}
</body>
</html>
